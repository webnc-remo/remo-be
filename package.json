{
  "name": "remo",
  "version": "1.0.0",
  "description": "ReMo, Typescript, Postgres, TypeORM",
  "author": "ReMo team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "nest build",
    "start:hmr": "node dist/main.hmr.ts",
    "start:dev": "nest start -w",
    "start:prod": "node dist/main.js",
    "watch:dev": "VERSION=$npm_package_version TZ='UTC' nest start --debug --watch",
    "worker:dev": "VERSION=$npm_package_version TZ='UTC' nest start --config nest-worker-cli.json --watch",
    "webpack": "webpack --config webpack.config.js --progress",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d ./ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./ormconfig.ts",
    "migration:create": "npm run typeorm migration:create",
    "new": "hygen new",
    "migration:revert": "npm run typeorm migration:revert",
    "schema:drop": "npm run typeorm schema:drop",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "npm docs:build && gh-pages -d .vuepress/dist",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.1",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nestjs-cls": "^4.3.0",
    "parse-duration": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "~0.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.8",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.13.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-canonical": "^4.18.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "~15.2.5",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "_moduleAliases": {
    "@modules": "dist/modules",
    "@config": "dist/config",
    "@constants": "dist/constants",
    "@decorators": "dist/decorators",
    "@filters": "dist/filters",
    "@interceptors": "dist/interceptors",
    "@middlewares": "dist/middlewares",
    "@common": "dist/common",
    "@guards": "dist/guards",
    "@interfaces": "dist/interfaces"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
